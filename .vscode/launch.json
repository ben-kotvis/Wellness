{
    "version": "0.2.0",
    "compounds": [
    ],
    "configurations": [
        {
            "name": "Launch and Debug",
            "type": "blazorwasm",
            "preLaunchTask": "build",
            "request": "launch",
            "url": "https://localhost:44353"
        },
        {
            "name": "API",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build api",
            "program": "${workspaceFolder}/Wellness.Api/bin/Debug/net5.0/Wellness.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Wellness.Api",
            "stopAtEntry": false,
            "console": "internalConsole",
        },
        {
            "name": "Blazor Client",
            "type": "blazorwasm",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Wellness.Client/bin/Debug/net5.0/Wellness.Client.dll",
            "cwd": "${workspaceFolder}/Wellness.Client",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "url": "https://localhost:44353"
        }
    ]
}