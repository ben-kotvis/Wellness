{
    "version": "0.2.0",
    "compounds": [
        {
            "name": "Compound",
            "configurations": ["Blazor Server","API"],
            "preLaunchTask": "build"
        }
    ],
    "configurations": [
        {
            "name": "API",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build api",
            "program": "${workspaceFolder}/Wellness.Api/bin/Debug/net5.0/Wellness.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Wellness.Api",
            "stopAtEntry": false,
            "console": "internalConsole",
            "url":"https://localhost:44354"
        },
        {
            "name": "Blazor Server",
            "hosted":true,
            "type": "blazorwasm",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Wellness.Server/bin/Debug/net5.0/Wellness.Server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Wellness.Server",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "url": "https://localhost:44353"
        }
    ]
}